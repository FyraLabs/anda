{
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "anda",
      "database": "anda",
      "username": "postgres",
      "password": "example"
    }
  ],
  "rust-analyzer.checkOnSave.command": "clippy",
  "search.useGlobalIgnoreFiles": true,
  "files.exclude": {
    "**/.flatpak-builder": true
  },
  "files.watcherExclude": {
    "**/.flatpak-builder/**": true
  },
  "dotenv.enableAutocloaking": false,
  "rust-analyzer.files.excludeDirs": [
    ".flatpak-builder"
  ],
  "files.associations": {
    "*.tcc": "c",
    "ostream": "c",
    "cstdio": "c"
  }
}
